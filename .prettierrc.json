{
  "trailingComma": "es5",
  "singleQuote": true,
  "printWidth": 120,
  "tabWidth": 2,
  "useTabs": false,
  "arrowParens": "always",
  "endOfLine": "lf",
  "semi": true,
  "overrides": [
    {
      "files": "*.md",
      "options": {
        "singleQuote": false,
        "quoteProps": "preserve"
      }
    }
  ]
}